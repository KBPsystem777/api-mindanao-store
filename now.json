{
  "name": "mindanao-store-api",
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node-server" }],
  "routes": [
    {
      "src": "/api/users",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/users/add",
      "dest": "/index.js",
      "methods": ["POST"]
    }
  ]
}
